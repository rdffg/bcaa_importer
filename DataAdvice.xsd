<xs:schema
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:da="http://data.bcassessment.ca/DataAdvice/Formats/DAX/DataAdvice.xsd"
    xmlns="http://data.bcassessment.ca/DataAdvice/Formats/DAX/DataAdvice.xsd"
    targetNamespace="http://data.bcassessment.ca/DataAdvice/Formats/DAX/DataAdvice.xsd"
    elementFormDefault="qualified">
  <xs:annotation>
    <xs:documentation>
      Defines the structure of a Data Advice XML document.
    </xs:documentation>
  </xs:annotation>

  <!--
    ******************************************************
    Data Advice Document.
    -->

  <xs:element
      name="DataAdvice"
      type="DataAdvice">
    <xs:annotation>
      <xs:documentation>
        The root element of a Data Advice XML document. The content of this
        element represents an entire Data Advice XML delivery for a single order.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:complexType
      name="DataAdvice">
    <xs:annotation>
      <xs:documentation>
        Represents an entire Data Advice XML delivery for a single order.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element
          name="RollYear"
          type="xs:integer"
          minOccurs="1"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The roll year parameter used to generate the Data Advice XML document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="OwnershipYear"
          type="xs:integer"
          minOccurs="1"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The ownership year parameter used to generate the Data Advice XML document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="RunType"
          type="RunType"
          minOccurs="1"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The run type parameter used to generate the Data Advice XML document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="StartDate"
          type="xs:date"
          minOccurs="1"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The start date of the reporting period for the Data Advice XML document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="EndDate"
          type="xs:date"
          minOccurs="1"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The end date of the reporting period for the Data Advice XML document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="AssessmentAreas"
          type="AssessmentAreaCollection"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Represents a collection of BCA assessment area folio groups.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="ReportSummary"
          type="DeliverySummary"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The total folio count and value summaries for the entire Data Advice XML document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute
        name="Version"
        type="Version"
        use="required"
        fixed="1.8">
      <xs:annotation>
        <xs:documentation>
          Identifies the version of the XML schema that defines the structure
          of the Data Advice XML document.
        </xs:documentation>
        <xs:documentation>
          Note: Documents that conform to this schema shall have a value of "1.8"
          for this attribute.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute
        name="RequestID"
        type="UniqueID"
        use="required">
      <xs:annotation>
        <xs:documentation>
          A unique identifier for the request that specified the order parameters.
          The value is intended for BCA internal purposes only.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute
        name="OrderID"
        type="UniqueID"
        use="required">
      <xs:annotation>
        <xs:documentation>
          A unique identifier for the order that generated the Data Advice XML document.
          The value is intended for BCA internal purposes only.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute
        name="RunDate"
        type="xs:date"
        use="required">
      <xs:annotation>
        <xs:documentation>
          The date when the order was run to generate the Data Advice XML document.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:simpleType
      name="Version">
    <xs:annotation>
      <xs:documentation>
        Represents a version number of one to four integers separated by periods (e.g. 1.0).
      </xs:documentation>
      <xs:documentation>
        Version numbers are assumed to be ordered according to standard conventions.
        If "x" and "y" are version numbers where "x" precedes "y" in sort order, then
        "x" shall identify a version that is historically older than the version identified by "y".
        No additional interpretation of a version number is currently defined.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction
        base="xs:string">
      <xs:pattern
          value="[0-9]+(\.[0-9]+){0,3}"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType
      name="RunType">
    <xs:annotation>
      <xs:documentation>
        Represents a code indicating the run type that generated the Data Advice XML document.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction
        base="xs:string">
      <xs:enumeration value="COMP">
        <xs:annotation>
          <xs:documentation>
            Indicates a Completed Roll run type.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="REVD">
        <xs:annotation>
          <xs:documentation>
            Indicates a Revised Roll run type.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="RVSP">
        <xs:annotation>
          <xs:documentation>
            Indicates a Revised + Supplementary Roll run type.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="GRNT">
        <xs:annotation>
          <xs:documentation>
            Indicates a Grant Roll run type.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SUPP">
        <xs:annotation>
          <xs:documentation>
            Indicates a Supplementary Cycle run type.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="PAAB">
        <xs:annotation>
          <xs:documentation>
            Indicates a Property Assessment Appeal Board (PAAB) Roll run type.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="DECISION">
        <xs:annotation>
          <xs:documentation>
            Indicates a Decision Roll run type.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="MONTHLY">
        <xs:annotation>
          <xs:documentation>
            Indicates a Monthly run type.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="WEEKLY">
        <xs:annotation>
          <xs:documentation>
            Indicates a Weekly run type.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="OTHER">
        <xs:annotation>
          <xs:documentation>
            Indicates a run type for a user-specified date range.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType
      name="DeliverySummary">
    <xs:annotation>
      <xs:documentation>
        Represents total folio counts and value summaries for an entire Data Advice XML document.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element
          name="TotalFolioCount"
          type="xs:integer"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The total number of folios included in the Data Advice XML document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="TaxableFolioCount"
          type="xs:integer"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The number of taxable folios included in the Data Advice XML document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="TaxExemptFolioCount"
          type="xs:integer"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The number of exempt folios included in the Data Advice XML document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="TotalGrossValues"
          type="FolioGroupValues"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The summary of gross (General) values for the folio group.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="TotalTaxExemptValues"
          type="FolioGroupValues"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The summary of tax-exempt (General) values for the folio group.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="TotalNetValues"
          type="FolioGroupValues"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The summary of net (General) values for the folio group.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="GeneralValues"
          type="PropertyClassValuesCollection"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Values summarized by property class and sub-class for general purposes.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="BCTransitValues"
          type="PropertyClassValuesCollection"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Values summarized by property class and sub-class for BC Transit purposes.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="SchoolValues"
          type="PropertyClassValuesCollection"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Values summarized by property class and sub-class for school purposes.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="AmendmentReasonCounts"
          type="AmendmentReasonCountCollection"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Total folio counts by amendment reason.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="DeleteReasonCounts"
          type="DeleteReasonCountCollection"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Total folio counts by delete reason.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType
      name="AmendmentReasonCountCollection">
    <xs:annotation>
      <xs:documentation>
        Represents a collection of folio counts by amendment reason.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element
          name="AmendmentReasonCount"
          type="AmendmentReasonCount"
          minOccurs="0"
          maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            The folio count for a single amendment reason.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType
      name="AmendmentReasonCount">
    <xs:annotation>
      <xs:documentation>
        Represents a count of folios for a particular amendment type.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element
          name="AmendmentReasonCode"
          type="LookupCode"
          minOccurs="1"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            A code indicating the amendment type.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
        name="AmendmentReasonDescription"
        type="String255"
        minOccurs="0"
        maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            A short description about the amendment reason.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="FolioCount"
          type="xs:integer"
          minOccurs="1"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The folio count for the amendment type.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType
      name="DeleteReasonCountCollection">
    <xs:annotation>
      <xs:documentation>
        Represents a collection of folio counts by delete reason.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element
          name="DeleteReasonCount"
          type="DeleteReasonCount"
          minOccurs="0"
          maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            The count of folio deletes for a particular delete reason.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType
      name="DeleteReasonCount">
    <xs:annotation>
      <xs:documentation>
        Represents a count of folio for a particular delete reason.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element
          name="DeleteReasonCode"
          type="LookupCode"
          minOccurs="1"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            A code indicating the delete reason.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="DeleteReasonDescription"
          type="String255"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            A short description about the delete reason.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="FolioCount"
          type="xs:integer"
          minOccurs="1"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The folio count for the delete reason.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <!--
    *******************************************************
    Assessment Area and Jurisdiction Folio Groups.
    -->

  <xs:complexType
      name="AssessmentAreaCollection">
    <xs:annotation>
      <xs:documentation>
        Represents a collection of BCA assessment area folio groups.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element
          name="AssessmentArea"
          type="AssessmentArea"
          minOccurs="0"
          maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            A folio group for a single BCA assessment area.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType
      name="AssessmentArea">
    <xs:annotation>
      <xs:documentation>
        Represents a folio group for a single BCA assessment area.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element
          name="AssessmentAreaCode"
          type="AssessmentAreaCode"
          minOccurs="1"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The BCA code that identifies the assessment area.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="AssessmentAreaDescription"
          type="String255"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The full name/description of the BCA assessment area.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="Jurisdictions"
          type="JurisdictionCollection"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The collection of jurisdiction folio groups for the BCA assessment area.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="AreaSummary"
          type="DeliverySummary"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Total folio count and value summaries for the BCA assessment area.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType
      name="AssessmentAreaCode">
    <xs:annotation>
      <xs:documentation>
        Represents a BCA code that identifies an assessment area.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction
        base="xs:string">
      <xs:maxLength
          value="16"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType
      name="JurisdictionCollection">
    <xs:annotation>
      <xs:documentation>
        Represents a collection of jurisdiction folio groups.
      </xs:documentation>
    </xs:annotation>

    <xs:sequence>
      <xs:element
          name="Jurisdiction"
          type="Jurisdiction"
          minOccurs="0"
          maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            A folio group for a single jurisdiction.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>

  </xs:complexType>

  <xs:complexType
      name="Jurisdiction">
    <xs:annotation>
      <xs:documentation>
        Represents a folio group for a single jurisdiction.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element
          name="JurisdictionCode"
          type="JurisdictionCode"
          minOccurs="1"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The BCA code that identifies the jurisdiction.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="JurisdictionDescription"
          type="String255"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The short name/description of the jurisdiction.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="FolioRecords"
          type="FolioRecordCollection"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The collection of folio data records for the jurisdiction.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="JurisdictionSummary"
          type="DeliverySummary"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Total folio count and value summaries for the jurisdiction.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType
      name="JurisdictionCode">
    <xs:annotation>
      <xs:documentation>
        Represents a BCA code that identifies a jurisdition.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction
        base="xs:string">
      <xs:maxLength
          value="16"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType
      name="FolioGroupValues">
    <xs:annotation>
      <xs:documentation>
        Represents value summaries for a folio group.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element
          name="LandValue"
          type="xs:decimal"
          minOccurs="1"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The sum of all land values reported for the folio group.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="ImprovementValue"
          type="xs:decimal"
          minOccurs="1"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The sum of all improvement(s) values reported for the folio group.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <!--
    *******************************************************
    Folio Records.
    -->

  <xs:complexType
      name="FolioRecordCollection">
    <xs:annotation>
      <xs:documentation>
        Represents a collection of folio data records.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element
          name="FolioRecord"
          type="FolioRecord"
          minOccurs="0"
          maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            A data record for a single folio.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType
      name="FolioRecord">

    <xs:annotation>
      <xs:documentation>
        Represents a data record for a single folio and its associated attributes.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>

      <xs:element
          name="RollNumber"
          type="FolioRollNumber"
          minOccurs="1"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The unique identifier for the specific folio within its jurisdiction.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="FolioStatus"
          type="FolioLookupCodeItem"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Indicates the status of the folio at the time the Data Advice XML document was generated.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="FolioStatusDescription"
          type="FolioString255Item"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The short name/description of the folio status.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="FolioAction"
          type="FolioAction"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Specifies the folio action: Add, Delete.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="FolioAddresses"
          type="FolioAddressCollection"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The collection of addresses associated with the folio.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="OwnershipGroups"
          type="OwnershipGroupCollection"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The collection of ownership groups associated with the folio.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="LegalDescriptions"
          type="LegalDescriptionCollection"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The collection of legal descriptions associated with the folio.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="FolioDescription"
          type="FolioDescription"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Describes general characteristics of the folio.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="Sales"
          type="SaleCollection"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The collection of sales associated with the folio.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="Values"
          type="PropertyValues"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The land and improvement values for a folio.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element
          name="Amendments"
          type="FolioAmendmentCollection"
          minOccurs="0"
          maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The collection of amendments associated with the folio.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
